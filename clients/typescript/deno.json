{
  "name": "@mieweb/ozwellai",
  "version": "1.0.2",
  "description": "Official TypeScript/JavaScript client for OzwellAI API - OpenAI-compatible chat completions, embeddings, and file operations",
  "license": "Apache-2.0",
  "lock": false,
  "imports": {
    "zod": "npm:zod@^3.22.4"
  },
  "tasks": {
    "test": "deno test --config deno.json --allow-net test/**/*.deno.test.ts"
  },
  "compilerOptions": {
    "lib": ["deno.window"],
    "strict": true
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "publish": {
    "exclude": [
      "test/",
      "dist/",
      "node_modules/",
      "*.config.*",
      "tsconfig*.json"
    ]
  }
}
